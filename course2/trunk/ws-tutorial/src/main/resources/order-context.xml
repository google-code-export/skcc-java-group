<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cxf="http://cxf.apache.org/core"
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://cxf.apache.org/core http://cxf.apache.org/schemas/core.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">
	<cxf:bus>
		<cxf:features>
			<cxf:logging />
		</cxf:features>
	</cxf:bus>
	<jaxws:server address="http://localhost:8001/OrderService">
		<jaxws:serviceBean>
			<bean class="com.skcc.tutorial.ws.OrderServiceImpl" />
		</jaxws:serviceBean>
	</jaxws:server>

	<jaxws:client id="orderServiceClient" serviceClass="com.skcc.tutorial.ws.OrderService"
		address="http://localhost:8001/OrderService">
	</jaxws:client>
</beans>
